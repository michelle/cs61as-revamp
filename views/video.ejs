<div id="centered">
  <% for (var i = 0; i < videos.length; i += 1) { var video = videos[i];%>
    <h1><%-video.name%></h1>
    <% if (showControls) { %>
    <% if (video.isCompleted) {%>
      <div class="note"><div class="padding">You have watched this video.</div></div>
    <%} else {%>
      <form method="post" action="/webcast/<%-currentLesson.number%>/<%-videoId%>">
      <button>Mark Completed!</button>
      </form>
    <% } %>
    <% } %>
    <iframe width="420" height="315" src="http://www.youtube.com/embed/<%-video.url%>?wmode=transparent" frameborder="0" wmode="Opaque" allowfullscreen></iframe>
  <% } %>
</div>
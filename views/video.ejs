<div id="centered">
  <% if (!byurl) { %>
    <% for (var i = 0; i < vids.length; i += 1) { var video = vids[i];%>
      <h1><%-video.name%></h1>
      <iframe width="420" height="315" src="http://www.youtube.com/embed/<%-video.url%>?wmode=transparent" frameborder="0" wmode="Opaque" allowfullscreen></iframe>
    <% } %>
  <% } else {%>
    <iframe width="420" height="315" src="http://www.youtube.com/embed/<%-url%>?wmode=transparent" frameborder="0" wmode="Opaque" allowfullscreen></iframe>
  <% } %>
</div>
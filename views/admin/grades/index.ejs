<div id="centered">
  <div id="lesson" class="noselect">
    <h2>Grade Administration</h2>
  </div>
  <div id="lefttriangle"></div>
  <br>
  <div class="contentpadding">
    <div class="formbox">
      <h3>Enter grade for a user (<%-users.length %> total)</h3>
      <% for (var i = 0; i < users.length; i += 1) { var user = users[i];%>
        <span class="left boxy noselect displaymargin">username: <%-user.username %></span>
        <a class="right linkboxy displaymargin" href="/admin/grades/<%-user.username%>">(Enter Grade)</a>
      <% } %> <div class="clear"></div>
    </div>
  </div>
</div>